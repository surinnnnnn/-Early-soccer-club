generator client {
  provider = "prisma-client-js"
  output = "./generated/gameDataClient"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_GAME")
}

model agents {
  agent_id        Int        @id
  agent_name      String     @unique 
  attack          Int        @default(10)
  defense         Int        @default(10)
  position        String

} 

